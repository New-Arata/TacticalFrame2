{
    "multipart": [
        {   "apply": { "model": "tf:pipe_scaffold_post" }},
        {   "when": { "north": true },
            "apply": { "model": "tf:pipe_scaffold_side", "uvlock": true }
        },
        {   "when": { "east": true },
            "apply": { "model": "tf:pipe_scaffold_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "tf:pipe_scaffold_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "tf:pipe_scaffold_side", "y": 270, "uvlock": true }
        }
    ]
}
